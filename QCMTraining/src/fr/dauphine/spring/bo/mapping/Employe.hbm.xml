<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="fr.dauphine.spring.bo">
	<class name="Employe" table="EMPLOYE">
		<!-- Clé générée automatiquement par Hibernate -->
		<id name="id" type="string">
			<column name="ID_EMPLOYE" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<timestamp name="version" column="VERSION" unsaved-value="null" />
		<property name="nom">
			<column name="NOM" length="50" not-null="true" />
		</property>
		<property name="prenom">
			<column name="PRENOM" length="50" />
		</property>
		<many-to-one name="societe" column="ID_SOCIETE" class="fr.dauphine.spring.bo.Societe" cascade="none" />
	</class>
</hibernate-mapping>
