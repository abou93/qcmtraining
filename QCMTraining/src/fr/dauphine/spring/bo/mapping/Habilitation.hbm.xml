<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="fr.dauphine.spring.bo">
	<class name="Habilitation" table="HABILITATION">
		<!-- Clé générée automatiquement par Hibernate -->
		<id name="id" type="string">
			<column name="ID_HABILITATION" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<timestamp name="version" column="VERSION" unsaved-value="null" />
		<property name="libelle">
			<column name="LIBELLE" length="50" not-null="true" />
		</property>
		<property name="code">
			<column name="CODE" length="8" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
