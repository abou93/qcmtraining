<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="fr.dauphine.spring.bo">
	<class name="Societe" table="SOCIETE">
		<!-- Clé générée automatiquement par Hibernate -->
        <id name="id" type="long" column="ID_SOCIETE" unsaved-value="0">
            <generator class="identity"/>
        </id>
		<timestamp name="version" column="VERSION" unsaved-value="null" />
		<property name="nom">
			<column name="NOM" length="50" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
